{
  "name": "skedgy",
  "version": "0.8.0",
  "description": "Periodically check for and queue work to be done!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/skedgy.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/skedgy/issues"
  },
  "homepage": "https://github.com/JimmyBoh/skedgy#readme",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/node": "10.9.4",
    "ava": "^5.1.0",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "debug": "^4.0.1"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js",
      "dist/example.js",
      "dist/lib/models.js"
    ]
  }
}
